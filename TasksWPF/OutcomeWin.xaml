<Window x:Class="TasksWPF.OutcomeWin"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TasksWPF"
        mc:Ignorable="d"
        Title="Outcome" Height="350" Width="500">

    <Grid Background="#FFE0CBE5" Margin="0">
        <Grid.RowDefinitions>
            <RowDefinition Height="Auto"/>
            <RowDefinition Height="*"/>
            <RowDefinition Height="Auto"/>
        </Grid.RowDefinitions>
        <TextBox x:Name="textBox" Margin="72,45,72,107" FontFamily="Cascadia Code SemiBold"/>
        <Button Background="#FF9FDCD1" HorizontalAlignment="Center" Click="saveReport_Click"
        Content="Save the report" Margin="0,267,0,54" FontFamily="Cascadia Code SemiBold" Width="140" RenderTransformOrigin="0.499,-0.801"/>

    </Grid>
</Window>
